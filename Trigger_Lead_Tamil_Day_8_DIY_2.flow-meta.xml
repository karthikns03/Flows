        <defaultConnectorLabel>Greater Than 200k</defaultConnectorLabel>
        <rules>
            <name>Less_Than_100K</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.AnnualRevenue</leftValueReference>
